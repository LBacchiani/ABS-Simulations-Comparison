interface WrapperScaleInterface {
  List<DeploymentComponent> getDeploymentComponent();
  Int getDeplTimes();
	Unit deploy();
	Unit undeploy();
  Int getDeployedInstances();
}

class WrapperScale(Scale1 s1, Scale2 s2, Scale3 s3, Int nInst) implements WrapperScaleInterface{

  Int deplTimes = 0;

  List<DeploymentComponent> getDeploymentComponent(){
    Int getDeplTimes = 0;

    List<DeploymentComponent> res = Nil;
    if(s3 != null) {res = s3.getDeploymentComponent();}
    else if(s2 != null){res = s2.getDeploymentComponent();}
    else {res = s1.getDeploymentComponent();}
    return res;
  }

  Unit deploy(){
    if(s3 != null){s3!deploy();}
    else if(s2 != null){s2!deploy();}
    else{s1!deploy();}
    deplTimes = deplTimes + 1;
  }

  Unit undeploy(){
    if(s3 != null){s3!undeploy();}
    else if(s2 != null){s2!undeploy();}
    else{s1!undeploy();}
    deplTimes = deplTimes - 1;
  }

  Int getDeplTimes() {return deplTimes;}

  Int getDeployedInstances() {return nInst;}
}

interface MonitorInterface {
  Unit insertScalingPolicy (List<Rat> scalingValues, WrapperScaleInterface scalingObj, String scalingName);
}

//this class has an active behaviour so the monitoring operations start when the object is created in an automatic way
class Monitor(DBInterface db, Rat windowDim, List<Pair<Rat,Rat>> microservicesParams, Rat initialCost, Rat initInstancesNumber) implements MonitorInterface {
  //dimension of the time window for monitoring controls
  Rat monitoringWindowDimension = windowDim;
	//number of messagges supported by the initial configuration
  List<Rat> baseConfig = list[1, //MR
                         1, //MP
                         /*Triple(0,rat(0.2)*windowDim,1), //HA
                         Triple(0,rat(0.2)*windowDim,1), //LA
                         Triple(0,rat(0.075)*windowDim,1),  //TA*/
                         2, //SA
                         1,  //VS
                         1, //AM
                         1, //IA
                         1,  //NSFW
                         1,  //IR
                         1]; //MA
  //name -> (list of (n instance), number of scaling done, wrapper object)
  Map<String, Triple<List<Rat>, Int, WrapperScaleInterface>> scalingPolicy = map[];
  Rat messagesSupported = 0;
  Int i = 10;
  Rat ksmall = 13;
  Rat kbig =  39;
  Map<String,Int> scalingTrace = map[];

  Unit insertScalingPolicy (List<Rat> scalingValues, WrapperScaleInterface scalingObj, String scalingName){
    scalingPolicy = put(scalingPolicy, scalingName, Triple(scalingValues, 0, scalingObj));
    scalingTrace = put(scalingTrace,scalingName,0);
  }

  List<Rat> vectorSum(List<Rat> v1, List<Rat> v2) {
    Int i = 0;
    List<Rat> res = list[];
    while(i < length(v1)) {
      Rat e1 = nth(v1,i);
      Rat e2 = nth(v2,i);
      res = appendright(res, e1 + e2);
      i = i + 1;
    }
    return res;
  }

  Rat findMin(List<Rat> microserviceInstances) {
    Rat min = 1000000000;
    Int i = 0;
    while(i < length(microserviceInstances)) {
      Pair<Rat,Rat> microservicesParam = nth(microservicesParams, i);
      Rat mcl = fst(microservicesParam);
      Rat mf = snd(microservicesParam);
      Rat n_inst = nth(microserviceInstances,i);
      Rat handled_frequency = n_inst * mcl / mf;
      if(handled_frequency < min){min = handled_frequency;}
      i = i + 1;
    }
    return min;
  }

  Map<String,Int> createUndeployedPolicyMap(List<String> keys) {
    Map<String,Int> policies = map[];
    foreach(k in keys) {policies = put(policies, k, 0);}
    return policies;
  }

  String findOrchestration(Rat target, List<Rat> currentConfiguration) {
    String keyFound = "Scale3";
    Rat dist = 99999999999;
    foreach(entry in entries(scalingPolicy)) {
      String key = fst(entry);
      Triple<List<Rat>, Int, WrapperScaleInterface> val = snd(entry);
      List<Rat> orchestration = fstT(val);
      List<Rat> config_res = await this!vectorSum(currentConfiguration, orchestration);
      Rat freqMin = await this!findMin(config_res);
      if(freqMin - target > 0 && dist >= freqMin - target){
        dist = freqMin - target;
        keyFound = key;
      }
    }
    return keyFound;
  }

  Pair<Map<String,Int>,Map<String,Int>> findBestConfiguration(Rat totalMessagesArrived) {
    List<String> keys = elements(keys(scalingPolicy));
    Map<String,Int> toDeplyPolicies = await this!createUndeployedPolicyMap(keys);
    Map<String,Int> printablepolicies = await this!createUndeployedPolicyMap(keys);
    List<Rat> targetConfiguration = baseConfig;
    messagesSupported = await this!findMin(baseConfig);
    while(totalMessagesArrived - messagesSupported > 0) {
      String policyName = await this!findOrchestration(totalMessagesArrived, targetConfiguration);
      Triple<List<Rat>, Int, WrapperScaleInterface> val = fromJust(lookup(scalingPolicy, policyName));
      List<Rat> n_instance_list = fstT(val);
      targetConfiguration = await this!vectorSum(targetConfiguration,n_instance_list);
      messagesSupported = await this!findMin(targetConfiguration);
      toDeplyPolicies = put(toDeplyPolicies, policyName, fromJust(lookup(toDeplyPolicies, policyName)) + 1);
      printablepolicies = put(printablepolicies, policyName, fromJust(lookup(printablepolicies, policyName)) + 1);
      toDeplyPolicies = await this!addNeededPolicy(policyName, toDeplyPolicies);
    }
    return Pair(toDeplyPolicies,printablepolicies);
  }

  Map<String,Int> addNeededPolicy(String policyName, Map<String,Int> currentPoliciesConfig) {
    Map<String,Int> toDeplyPolicies = currentPoliciesConfig;
    Bool passed = False;
    foreach(key in elements(keys(scalingPolicy))) {
      if(key == policyName) {passed = True;}
      if(!passed) {
        Int oldVal = fromJust(lookup(toDeplyPolicies, key));
        toDeplyPolicies = put(toDeplyPolicies, key, oldVal + 1);
      }
    }
    return toDeplyPolicies;
  }

  String recordAction(Map<String,Int> configuration) {
    String toPrint = "";
    foreach(entry in entries(configuration)) {
      Int oldVal = fromJust(lookup(scalingTrace,fst(entry)));
      Int optVal = snd(entry);
      if(optVal > oldVal) {
        toPrint = toPrint + " " + fst(entry) + "-upx" + toString(optVal - oldVal);
      } else if(oldVal > optVal) {
        toPrint = toPrint + " " + fst(entry) + "-downx" + toString(oldVal - optVal);
      }
      scalingTrace = put(scalingTrace,fst(entry),optVal);
    }
    return toPrint;
  }

  Unit reconfigureSystem(Map<String,Int> configuration) {
    foreach(en in entries(configuration)) {
      WrapperScaleInterface s = trdT(fromJust(lookup(scalingPolicy,fst(en))));
      Int oldVal = sndT(fromJust(lookup(scalingPolicy,fst(en))));
      Int optVal = snd(en);
      if(optVal > oldVal) {
        while(oldVal != optVal) {
          s!deploy();
          oldVal = oldVal + 1;
        }
        scalingPolicy = put(scalingPolicy, fst(en), Triple(fstT(fromJust(lookup(scalingPolicy, fst(en)))), optVal, trdT(fromJust(lookup(scalingPolicy, fst(en))))));
      } else if(optVal < oldVal) {
        while(oldVal != optVal) {
          s!undeploy();
          oldVal = oldVal - 1;
        }
        scalingPolicy = put(scalingPolicy, fst(en), Triple(fstT(fromJust(lookup(scalingPolicy, fst(en)))), optVal, trdT(fromJust(lookup(scalingPolicy, fst(en))))));
      }
    }
  }

  Rat computeTotalCost() {
    Rat totalCost = 0;
    foreach(triple in values(scalingPolicy)) {
      WrapperScaleInterface wrapper = trdT(triple);
      Rat scaleCost = await this!computeCostPerScaling(wrapper);
      Int deplTimes = await wrapper!getDeplTimes();
      scaleCost = scaleCost * deplTimes;
      totalCost = totalCost + scaleCost;
    }
    return totalCost;
  }

  Rat computeCostPerScaling(WrapperScaleInterface wrapper) {
    List<DeploymentComponent> dcs = await wrapper!getDeploymentComponent();
    Rat cost = 0;
    foreach(dc in dcs) {
      Rat singleCost =  await dc!getCostPerInterval();
      cost = cost + singleCost;
    }
    return cost;
  }

  Rat computeInstanceNumber() {
    Rat nInstances = 0;
    foreach(triple in values(scalingPolicy)) {
      WrapperScaleInterface wrapper = trdT(triple);
      Int n = await wrapper!getDeployedInstances();
      Int deplTimes = await wrapper!getDeplTimes();
      nInstances = nInstances + (n * deplTimes);
    }
    return nInstances;
  }

	// active behavior
	Unit run() {
    println("#X   Y");
    messagesSupported = await this!findMin(baseConfig);
    while(True){
      //wait the monitoring time
			duration(monitoringWindowDimension, monitoringWindowDimension);
      Float averageAnalysisTime = await db!returnAverageAnalysisTime();
      Rat completed = await db!messageCompleted();
      completed = completed / 10;
      Rat numberOfMessaggesInTheMonitoringWindow = await db!returnNumberOfMessaggesInTheMonitoringWindow();
      numberOfMessaggesInTheMonitoringWindow = numberOfMessaggesInTheMonitoringWindow / 10;
      Rat pending = await db!pendingMessages();
      pending = pending / 10;
      Rat rejected = await db!rejectedMessages();
      rejected = rejected / 10;
      Rat minBase = await this!findMin(baseConfig);
      String res = "";
      Rat totalCost = await this!computeTotalCost();
      Rat nInst = await this!computeInstanceNumber();
      nInst = nInst + initInstancesNumber;
      String toPrint = toString(i) + "  " + toString(float(100/3) * averageAnalysisTime) + " #MSG:" + toString(float(numberOfMessaggesInTheMonitoringWindow)) + " COMPLETED:" + toString(float(completed)) + " REJECTED: " + toString(float(rejected)) + " SUPP: " + toString(messagesSupported) + " COST: " + toString(float((totalCost + initialCost) / 1000)) + " INST: " + toString(nInst);
      if(numberOfMessaggesInTheMonitoringWindow - (messagesSupported - kbig) > ksmall || (messagesSupported - kbig)  - numberOfMessaggesInTheMonitoringWindow > ksmall){
        Pair<Map<String,Int>,Map<String,Int>> configurations = await this!findBestConfiguration(numberOfMessaggesInTheMonitoringWindow + kbig);
        Map<String,Int> configuration = fst(configurations);
        Map<String,Int> printableconfiguration = snd(configurations);
        await this!reconfigureSystem(configuration);
        res = await this!recordAction(printableconfiguration);
      }
      toPrint = toPrint +  res;
      i = i + 10;
      db!resetMetrics();
      println(toPrint);
    }
  }
}

